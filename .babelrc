        "production": {
            "presets": [
                "@babel/preset-env",
                "@babel/preset-react",
                "@emotion/babel-preset-css-prop"
            ],
            "plugins": [
                "emotion"
            ]
        },
        "development": {
            "presets": [
                "@babel/preset-env",
                "@babel/preset-react",
                "@emotion/babel-preset-css-prop"
            ],
            "plugins": [
                "emotion",
                "react-hot-loader/babel"
            ]
